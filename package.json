    "reducer",
    "react"
  ],
  "author": "Ofir Zeitoun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ofir-zeitoun/typed-pub-sub/issues"
  },
  "homepage": "https://github.com/ofir-zeitoun/typed-pub-sub#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7"
